<div class="mb-5 pt-3">
    <a href="/dashboard/create/"><button id="createBusiness" class="button is-primary is-pulled-right">Create Business</button></a>

    <h2 class="title pb-3">Dashboard: Your Business</h2>
    {{#if posts.length}}
</div>

{{#each posts as |post|}}
    <div>{{post.title}}</div>
        <a href="/dashboard/edit/{{post.id}}"><button class="button mb-5 ml-5 is-info">Edit Business:</button></a>
        </div>
    {{/each}}

{{else}}
<h5 class="pb-4 has-text-centered is-italic">You do not currently have a business</h5>

{{/if}}